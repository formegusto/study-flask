    "import pymysql\n",
    "\n",
    "rdb_conn = pymysql.connect(\n",
    "    host=\"localhost\",\n",
    "    port=3306,\n",
    "    user=\"root\",\n",
    "    passwd=\"root\",\n",
    "    database=\"blog_db\",\n",
    "    charset=\"utf8\"\n",
    ")\n",
    "\n",
    "cur = rdb_conn.cursor()\n",
    "SQL = 'SHOW TABLES;'\n",
    "print(cur.execute(SQL))\n",
    "\n",
    "rdb_conn.close()"
